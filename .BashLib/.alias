# creates .rvmrc
function mrc(){
    echo 'rvm use '$1'@'$2' --create' > .rvmrc;
}
#imports mysql server
function import_sql(){
    mysql -u root $2 < $1
}
#opens rails server on port $1
function rsv(){
    rails s -p $1
}

